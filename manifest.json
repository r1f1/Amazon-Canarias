{
	"manifest_version": 2,
	"name": "Amazon Canarias",
	"version": "1.2",
	"author": "Rafa",
	"content_scripts": [
		{
			"matches": [
				"http://www.amazon.es/*",
				"https://www.amazon.es/*"
			],
			"js": ["contents.js"]
		}
	],
	"icons": {
			"16": "icon16.png",
			"48": "icon48.png",
			"128": "icon128.png"
		}
}